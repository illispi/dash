{
  "name": "example-with-tailwindcss",
  "type": "module",
  "scripts": {
    "dev": "vinxi dev --host",
    "build": "vinxi build",
    "start": "vinxi start --host",
    "encrypt": "tsx ./src/utils/encrypt.ts",
    "decrypt": "tsx ./src/utils/decrypt.ts",
    "migrate:latest": "tsx ./src/utils/migrate.ts",
    "migrate:down": "tsx ./src/utils/migrate.ts down",
    "new:migration": "tsx ./src/utils/migrationFilename.ts",
    "sql-types": "tsx kysely-codegen --dialect postgres --camel-case"
  },
  "dependencies": {
    "@kobalte/core": "^0.13.6",
    "@solidjs/router": "^0.14.5",
    "@solidjs/start": "^1.0.6",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "kysely": "^0.27.4",
    "kysely-codegen": "^0.16.8",
    "pg": "^8.13.0",
    "postcss": "^8.4.47",
    "solid-js": "^1.8.22",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.12",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "vinxi": "^0.4.3"
  },
  "engines": {
    "node": ">=18"
  }
}